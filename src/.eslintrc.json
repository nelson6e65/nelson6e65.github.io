{
    "extends": [
        "eslint:recommended",
        "vue"
    ],
    "plugins": [
        "html"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 8
    },
    "env": {
        "node": true,
        "browser": true
    },
    "globals": {
        "$": false,
        "jQuery": false
    },
    "rules": {
        "no-console": 1,
        "no-unused-vars": 1,
        "strict": [1, "global"]
    }
}
